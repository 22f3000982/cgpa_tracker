{
  "version": 2,
  "builds": [
    {
      "src": "./api/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/app.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production"
  },
  "functions": {
    "api/app.py": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
